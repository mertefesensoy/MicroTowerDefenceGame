git_url(ENV.fetch("MATCH_GIT_URL"))
storage_mode("git")

type("appstore")

app_identifier(["com.mertefesensoy.MicroTowerDefenceGame"])
team_id(ENV.fetch("DEVELOPER_PORTAL_TEAM_ID"))

# Recommended settings
readonly(false) # lane can override to true for beta
clone_branch_directly(true)

# Use basic auth header if you're using MATCH_GIT_BASIC_AUTHORIZATION
git_basic_authorization(ENV["MATCH_GIT_BASIC_AUTHORIZATION"]) if ENV["MATCH_GIT_BASIC_AUTHORIZATION"]
